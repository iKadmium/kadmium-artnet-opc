<?xml version="1.0" encoding="UTF-8"?>
<programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="programs.xsd">
    <program>
        <hsb hue="0.0" saturation="0.0" brightness="0.0"/>
        <effects/>
    </program>

    <program>
        <hsb hue="0.0" saturation="1.0" brightness="1.0"/>
        <effects>
            <shift property="hue" pixelShiftDelta="0.01" horizontal="true" leftToRight="true" topToBottom="true"/>
            <shift property="brightness" pixelShiftDelta="0.15" horizontal="false" leftToRight="false" topToBottom="true"/>
        </effects>
    </program>

    <program>
        <hsb hue="0.0" saturation="1.0" brightness="1.0"/>
        <effects>
            <wave property="brightness" pixelShiftDelta="0.5" horizontal="true" vertical="true" speedMultiplier="5"/>
        </effects>
    </program>

    <program>
        <hsb hue="0.0" saturation="1.0" brightness="1.0"/>
        <effects>
            <shift property="saturation" pixelShiftDelta="0.05" horizontal="false" leftToRight="true" topToBottom="true"/>
            <shift property="brightness" pixelShiftDelta="0.15" horizontal="false" leftToRight="false" topToBottom="true" cutoffMin="0.95" cutoffMax="1.0" />
        </effects>
    </program>
</programs>